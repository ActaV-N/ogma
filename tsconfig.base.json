{
    "compilerOptions": {
      "target": "es2019",
      "module": "commonjs",
      "declaration": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "composite": true
    },
    "exclude": ["**/node_modules", "**/dist"]
  }
  